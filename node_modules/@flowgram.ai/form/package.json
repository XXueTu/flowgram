  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "fast-equals": "^2.0.0",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.2",
    "@flowgram.ai/reactive": "0.2.4",
    "@flowgram.ai/utils": "0.2.4"
  },
  "devDependencies": {
    "@testing-library/react": "^12",
    "@types/lodash": "^4.14.137",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitest/coverage-v8": "^0.32.0",
    "eslint": "^8.54.0",
    "tsup": "^8.0.1",
    "typescript": "^5.0.4",
    "vitest": "^0.34.6",
    "@flowgram.ai/eslint-config": "0.2.4",
    "@flowgram.ai/ts-config": "0.2.4"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "publishConfig": {
